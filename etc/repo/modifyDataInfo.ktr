        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#x2f;&#x2f; Script here&#xa;&#xa;import org.ossim.omar.hibernate.domain.io.DataInfo&#xa;import org.ossim.omar.hibernate.domain.io.XmlIoHints&#xa;&#xa;&#xa;def changeString&#x28;def input&#x29;&#xa;&#x7b;&#xa;	input.replaceFirst&#x28;&#x22;z&#x3a;&#x22;, &#x22;&#x2f;&#x22;&#x29;&#xa;&#x7d;&#xa;def dataInfo &#x3d; new DataInfo&#x28;&#x29;&#xa;&#xa;dataInfo.initFromString&#x28;oms_info, new XmlIoHints&#x28;&#x29;&#x29;&#xa;&#xa;def rasterDataSets &#x3d; dataInfo.rasterDataSets&#xa;rasterDataSets&#x3f;.each&#x7b;rasterDataSet-&#x3e;&#xa;	&#x2f;&#x2f; modify all raster files&#xa;	rasterDataSet.rasterFiles.each&#x7b;rasterFile-&#x3e;&#xa;		rasterFile.name &#x3d; changeString&#x28;rasterFile.name&#x29;&#xa;	&#x7d;&#xa;&#xa;	&#x2f;&#x2f; modify filename in rasterentries&#xa;	rasterDataSet.rasterEntries.each&#x7b;rasterEntry-&#x3e;&#xa;		rasterEntry.filename &#x3d;  changeString&#x28;rasterEntry.filename&#x29;&#xa;&#xa;		rasterEntry.rasterEntryFiles&#x3f;.each&#x7b;rasterEntryFile-&#x3e;&#xa;			rasterEntryFile.name &#x3d; changeString&#x28;rasterEntryFile.name&#x29;&#xa;		&#x7d;&#xa;	&#x7d;&#xa;&#x7d;&#xa;&#xa;oms_info &#x3d; dataInfo.xml&#xa;</jsScript_script>
